                                <% }else{ %>
                                <input type="radio" name="question1" id="a2" value="b">Mumbai </input>
                                <% } %>
                                <% if (question1value == 'c'){ %>
                                <input type="radio" name="question1" id="a3" value="c" checked>Aligarh</input>
                                <% }else{ %>
                                <input type="radio" name="question1" id="a3" value="c">Aligarh</input>
                                <% } %>
                                <% if (question1value == 'd'){ %>
                                <input type="radio" name="question1" id="a4" value="d" checked>Bengaluru</input>
                                <% }else{ %>
                                <input type="radio" name="question1" id="a4" value="d">Bengaluru</input>
                                <% } %>

                            </td>
                        </tr>
                        <tr class="feature clearfix">
                            <td class="feature-category">
                                What is the capital of United States ?
                            </td>
                            <td class="feature-name">
                                <% if (question2value == 'a'){ %>
                                <input type="radio" name="question2" id="b1" value="a" checked>New York</input>
                                <% }else{ %>
                                <input type="radio" name="question2" id="b1" value="a">New York</input>
                                <% } %>
                                <% if (question2value == 'b'){ %>
                                <input type="radio" name="question2" id="b2" value="b" checked>Stamford</input>
                                <% }else{ %>
                                <input type="radio" name="question2" id="b2" value="b">Stamford</input>
                                <% } %>
                                <% if (question2value == 'c'){ %>
                                <input type="radio" name="question2" id="b3" value="c" checked>Washington DC</input>
                                <% }else{ %>
                                <input type="radio" name="question2" id="b3" value="c">Washington DC</input>
                                <% } %>
                                <% if (question2value == 'd'){ %>
                                <input type="radio" name="question2" id="b4" value="d" checked>New Heaven</input>
                                <% }else{ %>
                                <input type="radio" name="question2" id="b4" value="d">New Heaven</input>
                                <% } %>

                            </td>
                        </tr>
                        <tr class="feature clearfix">
                            <td class="feature-category">
                                <input type="submit"/>
                            </td>

                        </tr>
                    </table>
                </form>
            </div>
        </div>
        <!--- END GRID -->
