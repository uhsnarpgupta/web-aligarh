                <div class="std-container std-padding std-black">
                    <h4>Test Begins</h4>
                </div>
                <div id="finalscore" name="finalscore">
                    <h4 style="text-align: right;color: #990000;">Final Score is: <%= score %>/2 <%= percentage %>%</h4>
                </div>
                <form method="post" action="/engtestpaper/getresult">
                    <table class="features-wrapper std-table std-striped std-bordered std-border std-hoverable std-white">
                        <tr class="feature clearfix">
                            <td class="feature-category">
                                What is the capital of India ?
                            </td>
                            <td class="feature-name">
                                <% if (question1value == 'a'){ %>
                                <input type="radio" name="question1" id="a1" value="a" checked>Delhi</input>
                                <% }else{ %>
